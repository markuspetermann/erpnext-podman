[Unit]
Description=ERPNext Frontend
After=erpnext-backend.service

[Container]
Pod=erpnext.pod
Image=localhost/erpnext-custom:version-15
Volume=%h/data/sites:/home/frappe/frappe-bench/sites:U
Environment=BACKEND=localhost:8000
Environment=SOCKETIO=localhost:9000
EnvironmentFile=%h/.env.erpnext
Exec=nginx-entrypoint.sh

[Service]
Restart=always
